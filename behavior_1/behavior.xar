                      <Box name="Trigonometry 1" id="18" localization="8" tooltip="Calculate robot distances." x="174" y="60">
                        <bitmap>media/images/box/box-python-script.png</bitmap>
                        <script language="4">
                          <content>
                            <![CDATA[import math

class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self)

    def onInput_onStart(self):
        self.xAxis = self.getParameter("X Axis")
        self.yAxis = self.getParameter("Y Axis")

        angle = math.radians(45) - math.atan(self.xAxis / self.yAxis)    # positive with left movement
        hypotenuse = math.sqrt(self.xAxis ** 2 + self.yAxis ** 2)    # Pythagorean theorem

        xMovement = - hypotenuse * math.cos(angle)
        yMovement = hypotenuse * math.sin(angle)

        radians = math.radians(-135)

        self.movement([xMovement, yMovement, radians])]]>
                          </content>
                        </script>
